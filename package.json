{
  "name": "virtuoso-design-system",
  "version": "1.1.0",
  "description": "A set of reusable elements that enable Virtru designers and Virtru engineers to create consistent Virtru products.",
  "author": "Virtru CI <ci@virtru.com>",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "auto": "auto",
    "build": "rollup --config config/rollup.config.js",
    "build:watch": "npm run build -- --watch --watch.include \"lib/**\"",
    "build:antDstories": "node ./scripts/convertAntdToStory.js",
    "clean": "rimraf dist/*",
    "prepublishOnly": "npm run build",
    "start": "npm run storybook",
    "test": "run-s test:*",
    "test-ci": "jest --ci --coverage",
    "test:js": "jest",
    "test:vrt": "build-storybook && loki --require-reference --reactUri file:./storybook-static",
    "storybook": "start-storybook -p 6006",
    "lint": "run-p lint:js lint:style lint:commit-msg:last",
    "lint:js": "eslint",
    "lint:style": "stylelint ./**/*.css",
    "lint:commit-msg:edit": "commitlint --edit",
    "lint:commit-msg:last": "commitlint --from HEAD~1 --to HEAD",
    "prettify": "eslint . --fix",
    "release": "npx node-env-run --env .env-auto --exec \"auto shipit\"",
    "postrelease": "storybook-to-ghpages --out .storybook-out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avkvirtru/virtuoso-design-system.git"
  },
  "keywords": [
    "Virtru",
    "design system",
    "React"
  ],
  "contributors": [
    "Arthur Klepchukov",
    "Ryan Schumacher"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avkvirtru/virtuoso-design-system/issues"
  },
  "homepage": "https://github.com/avkvirtru/virtuoso-design-system#readme",
